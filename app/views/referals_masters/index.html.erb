<p id="notice"><%= notice %></p>

<h1>Listing Referals Masters</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Refered user</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @referals_masters.each do |referals_master| %>
      <tr>
        <td><%= referals_master.user_id %></td>
        <td><%= referals_master.refered_user_id %></td>
        <td><%= referals_master.status %></td>
        <td><%= link_to 'Show', referals_master %></td>
        <td><%= link_to 'Edit', edit_referals_master_path(referals_master) %></td>
        <td><%= link_to 'Destroy', referals_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

