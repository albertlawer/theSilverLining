<%= simple_form_for(@request_master) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :customer_number %>
    <%= f.input :network %>
    <%= f.input :voucher_code %>
    <%= f.input :trans_type %>
    <%= f.input :item_desc %>
    <%= f.input :trans_id %>
    <%= f.input :amount %>
    <%= f.input :status %>
    <%= f.input :callback_status %>
    <%= f.input :resp_code %>
    <%= f.input :resp_desc %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
