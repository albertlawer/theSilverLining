<p id="notice"><%= notice %></p>

<h1>Listing Account Masters</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Avaliable balance</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_masters.each do |account_master| %>
      <tr>
        <td><%= account_master.user_id %></td>
        <td><%= account_master.avaliable_balance %></td>
        <td><%= account_master.status %></td>
        <td><%= link_to 'Show', account_master %></td>
        <td><%= link_to 'Edit', edit_account_master_path(account_master) %></td>
        <td><%= link_to 'Destroy', account_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account master', new_account_master_path %>
